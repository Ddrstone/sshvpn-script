{
    "outbounds": [
        {
            "protocol": "shadowsocks",
            "mux": {
                "concurrency": 8,
                "enabled": true
            },
            "settings": {
                "servers": [
                    {
                        "address": "IPVPS",
                        "port": 1111,
                        "method": "aes-128-gcm",
                        "email": "PASSWORD",
                        "password": "PASSWORD",
                        "level": 0
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "none"
            },
            "tag": "proxy"
        },
        {
            "protocol": "freedom",
            "settings": {},
            "tag": "direct"
        },
        {
            "protocol": "blackhole",
            "settings": {
                "response": {
                    "type": "http"
                }
            },
            "tag": "block"
        }
    ],
    "routing": {
        "domainStrategy": "AsIs",
        "rules": [
            {
                "inboundTag": [
                    "proxy"
                ],
                "outboundTag": "dns",
                "type": "field"
            },
            {
                "inboundTag": [
                    "http"
                ],
                "outboundTag": "proxy",
                "type": "field"
            }
        ]
